<% if user_signed_in? %>
    <ul>
        <% @users.each do |user|%>
            <% if user.id != current_user.id %>
                <li>
                <%= image_tag("images.png",
                class:"rounded-circle",height:"35") %> 
                <%= link_to user.name%>
                <%= link_to image_tag("f6bbced354652478430f121f13ca48b8.png", class:"rounded-circle",height:"35"), notifications_url(user.id), method: :post %>
                </li>
                <br>
            <%end%>
        <%end%>
    </ul>
<% else %>
    <% redirect_to new_user_session_path%>
<% end%>

